{"ast":null,"code":"import * as THREE from 'three';\nimport OrbitControls from 'three-orbitcontrols';\nexport default {\n  name: 'ThreeJSRenderer',\n  mounted() {\n    this.initThree();\n    this.controls = new OrbitControls(this.camera, this.renderer.domElement);\n  },\n  methods: {\n    initThree() {\n      this.scene = new THREE.Scene();\n      this.camera = new THREE.PerspectiveCamera(75, this.$refs.threeContainer.clientWidth / this.$refs.threeContainer.clientHeight, 0.1, 1000);\n      this.camera.position.z = 5;\n      this.renderer = new THREE.WebGLRenderer();\n      this.renderer.setSize(this.$refs.threeContainer.clientWidth, this.$refs.threeContainer.clientHeight);\n      this.$refs.threeContainer.appendChild(this.renderer.domElement);\n      const geometry = new THREE.BoxGeometry();\n      const material = new THREE.MeshBasicMaterial({\n        color: 0x00ff00\n      });\n      this.cube = new THREE.Mesh(geometry, material);\n      this.scene.add(this.cube);\n      this.animate();\n    },\n    animate() {\n      requestAnimationFrame(this.animate);\n      this.cube.rotation.x += 0.01;\n      this.cube.rotation.y += 0.01;\n      this.controls.update();\n      this.renderer.render(this.scene, this.camera);\n    }\n  }\n};","map":{"version":3,"names":["THREE","OrbitControls","name","mounted","initThree","controls","camera","renderer","domElement","methods","scene","Scene","PerspectiveCamera","$refs","threeContainer","clientWidth","clientHeight","position","z","WebGLRenderer","setSize","appendChild","geometry","BoxGeometry","material","MeshBasicMaterial","color","cube","Mesh","add","animate","requestAnimationFrame","rotation","x","y","update","render"],"sources":["/home/zhu/Documents/github/explorer_front/explorer_front_app/src/components/ThreeJSRenderer.vue"],"sourcesContent":["<template>\n  <div ref=\"threeContainer\"></div>\n</template>\n\n<script>\nimport * as THREE from 'three';\nimport OrbitControls from 'three-orbitcontrols';\n\nexport default {\n  name: