ate();\n        renderer.render(scene, camera);\n      };\n      animate();\n    },\n  },\n};\n</script>\n\n<style scoped>\n  div {\n    width: 100%;\n    height: 100vh;\n  }\n</style>"],"mappings":"AAKA,OAAO,KAAKA,KAAI,MAAO,OAAO;AAC9B,SAASC,aAAY,QAAS,8CAA8C;AAC5E,SAASC,SAAQ,QAAS,yCAAyC;AACnE;AACA;;AAGA,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAAI,CAAC,CAAC;EACb,CAAC;EACDC,OAAO,EAAE;IACPD,IAAIA,CAAA,EAAG;MACL,MAAME,SAAQ,GAAI,IAAI,CAACC,KAAK,CAACD,SAAS;;MAEtC;MACA,MAAME,KAAI,GAAI,IAAIT,KAAK,CAACU,KAAK,CAAC,CAAC;;MAE/B;MACA,MAAMC,MAAK,GAAI,IAAIX,KAAK,CAACY,iBAAiB,CAAC,EAAE,EAAEL,SAAS,CAACM,WAAU,GAAIN,SAAS,CAACO,YAAY,EAAE,GAAG,EAAE,IAAI,CAAC;MACzGH,MAAM,CAACI,QAAQ,CAACC,GAAG,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC;;MAE5B;MACA,MAAMC,QAAO,GAAI,IAAIjB,KAAK,CAACkB,aAAa,CAAC,CAAC;MAC1CD,QAAQ,CAACE,OAAO,CAACZ,SAAS,CAACM,WAAW,EAAEN,SAAS,CAACO,YAAY,CAAC;MAC/DP,SAAS,CAACa,WAAW,CAACH,QAAQ,CAACI,UAAU,CAAC;;MAE1C;MACA,MAAMC,QAAO,GAAI,IAAIrB,aAAa,CAACU,MAAM,EAAEM,QAAQ,CAACI,UAAU,CAAC;;MAE/D;MACA,MAAME,MAAK,GAAI,IAAIrB,SAAS,CAAC,CAAC;MAC9BqB,MAAM,CAACC,IAAI,CAAC,yBAAyB,EAAGC,QAAQ,IAAK;QACnD;QACA,MAAMC,aAAY,GAAI,IAAIC,aAAa,CAAC,CAAC;QACzCD,aAAa,CAACF,IAAI,CAAC,yBAAyB,EAAGI,OAAO,IAAK;UACzD,MAAMC,QAAO,GAAI,IAAI7B,KAAK,CAAC8B,iBAAiB,CAAC;YAAEC,GAAG,EAAEH;UAAQ,CAAC,CAAC;UAC9D,MAAMI,IAAG,GAAI,IAAIhC,KAAK,CAACiC,IAAI,CAACR,QAAQ,EAAEI,QAAQ,CAAC;UAC/CpB,KAAK,CAACyB,GAAG,CAACF,IAAI,CAAC;QACjB,CAAC,CAAC;MACJ,CAAC,CAAC;;MAEF;MACA,MAAMG,OAAM,GAAIA,CAAA,KAAM;QACpBC,qBAAqB,CAACD,OAAO,CAAC;QAC9Bb,QAAQ,CAACe,MAAM,CAAC,CAAC;QACjBpB,QAAQ,CAACqB,MAAM,CAAC7B,KAAK,EAAEE,MAAM,CAAC;MAChC,CAAC;MACDwB,OAAO,CAAC,CAAC;IACX;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}